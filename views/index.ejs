<h1><%= title %></h1>
<a href="/createRecipe">Create New Recipe</a>
// use a loop to show all recipe items at the very bottom.
<% recipes.forEach( function ( recipe ){ %>
  <p>
	<span>
      <a href="/view/<%= recipe._id %>" title="View full recipe item"><%= recipe.fn %></a>
    </span>
    <span>
      <a href="/edit/<%= recipe._id %>" title="Update this recipe item">Edit</a>
    </span>
    <span>
      <a href="/destroy/<%= recipe._id %>" title="Delete this recipe item">Delete</a>
    </span>
  </p>
<% }); %>
	